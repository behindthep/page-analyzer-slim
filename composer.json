{
  "name": "behindthpe/page-analyzer",
  "version": "0.0.1",
  "type": "project",
  "description": "Page Analyzer",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexey Kolvakh",
      "email": "alex.kolvah@mail.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "Page\\Analyzer\\": "src/",
      "Database\\Factories\\": "database/factories/"
    }
  },
  "require": {
    "slim/slim": "^4.9",
    "slim/psr7": "^1.7",
    "slim/http": "^1.4",
    "slim/flash": "^0.4.0",
    "php-di/php-di": "^7.0",
    "slim/php-view": "^3.4",
    "nesbot/carbon": "^3.8",
    "vlucas/valitron": "^1.4",
    "vlucas/phpdotenv": "^5.6",
    "guzzlehttp/guzzle": "^7.9",
    "imangazaliev/didom": "^2.0",
    "illuminate/support": "^11.41"
  },
  "require-dev": {
    "psy/psysh": "@stable",
    "phpstan/phpstan": "^2.1",
    "symfony/var-dumper": "^7.2",
    "squizlabs/php_codesniffer": "*",
    "phpstan/phpstan-phpunit": "^2.0",
    "phpstan/extension-installer": "^1.3"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
